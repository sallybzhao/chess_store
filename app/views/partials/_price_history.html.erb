<table id="ph-table">
  <thead> Price History </thead>
  <tr>
    <th> Price </th>
    <th> Start Date </th>
  </tr>

  <tbody>
    <% prices.chronological.each do |price| %>
      <tr>
        <td> <%= number_to_currency(price.price) %> </td>
        <td> <%= price.start_date.strftime('%m/%d/%y') %> </td>
      </tr>
    <% end %>
  </tbody> 
</table>